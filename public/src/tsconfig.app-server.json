{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../out-tsc/app",
    "baseUrl": "./",
    "module": "commonjs",
    "types": [
      "node"
    ],
    "paths": {
      "@app/*": [
        "app/*"
      ],
      "@shared/*": [
        "../../shared/*"
      ],
      "@server/*": [
        "../../server/*"
      ],
      "lazy_modules/admin/admin.browser": [
        "./app/lazy_modules/admin/server.browser"
      ],
      "ngx-quill-editor": [
        "server-mocks/modules/ngx-quill-editor/quillEditor.module.ts"
      ]
    }
  },
  "include": [
    "./**/*",
    "../../server/**/*.ts",
    "../../node_modules/ngx-quill-editor/quillEditor.component.ts",
    "../../node_modules/ngx-quill-editor/quillEditor.module.ts",
    "../../node_modules/ngx-quill-editor/index.ts"
  ],
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ],
  "angularCompilerOptions": {
    "entryModule": "app/app.server.module#AppServerModule"
  }
}