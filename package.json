  "scripts": {
    "ng": "ng",
    "init": "npm install && node config/scripts/init.js",
    "start": "ng serve --proxy-config config/ng/proxy.json",
    "start:server": "node dist/server",
    "build": "ng build",
    "build:app": "npm run build -- --prod --aot --output-hashing none",
    "build:app-server": "ng run app:server",
    "build:server": "webpack --config config/webpack/webpack.server.config --progress",
    "build:app-server:deprecated": "node node_modules/.bin/webpack --config config/webpack/webpack.app.config.js --env.aot --env.server --progress --colors"
  },
  "private": true,
