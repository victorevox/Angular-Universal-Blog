{
  "name": "mean-boilerplate",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/victorevox/Angular-Unoversal-Blog.git"
  },
  "contributors": [
    "Victor Heredia <victorevox@gmail.com>"
  ],
  "scripts": {
    "ng": "ng",
    "init": "npm install && node config/scripts/init.js",
    "start": "ng serve --proxy-config config/ng/proxy.json",
    "start:server": "node dist/server",
    "build": "ng build",
    "build:server-app": "ng build --prod --app 1 --output-hashing=false",
    "build:server": "npm run webpack:server && ng build --prod --app 1 --output-hashing=false",
    "build:client-and-server-bundles": "ng build --prod && npm run build:server-app",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "start:ssr": "ng build --watch && ng build --watch --app 1 && npm run webpack:server",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server": "webpack --config config/webpack/webpack.server.config.js --progress --colors",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.10",
    "@angular/common": "^5.2.10",
    "@angular/compiler": "^5.2.10",
    "@angular/core": "^5.2.10",
    "@angular/forms": "^5.2.10",
    "@angular/http": "^5.2.10",
    "@angular/platform-browser": "^5.2.10",
    "@angular/platform-browser-dynamic": "^5.2.10",
    "@angular/platform-server": "^5.2.10",
    "@angular/router": "^5.2.10",
    "@nguniversal/common": "^5.0.0",
    "@nguniversal/express-engine": "^5.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0",
    "@jaspero/ng-alerts": "^0.1.1",
    "@types/dotenv": "^4.0.3",
    "rxjs": "^5.5.10",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "angular2-notifications": "^1.0.1",
    "body-parser": "^1.18.2",
    "bootstrap": "4.1.0",
    "core-js": "^2.5.5",
    "crypto": "^1.0.1",
    "dotenv": "^5.0.1",
    "formidable": "^1.2.1",
    "http-status-codes": "^1.3.0",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.5",
    "method-override": "^2.3.10",
    "mongoose": "^5.0.15",
    "ng2-file-upload": "^1.3.0",
